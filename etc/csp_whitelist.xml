-->
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="font-src">
            <values>
                <value id="fonts.gstatic.com" type="host">https://fonts.gstatic.com/</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="fonts.googleapis.com" type="host">https://fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="cdnjs.cloudflare.com" type="host">https://cdnjs.cloudflare.com/</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="maps.google.com" type="host">https://maps.google.com/</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
