<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="pagebuilder_general"/>
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Template"
                   name="pagebuilder.import-template"
                   template="Goomento_PageBuilder::content/import_samples.phtml"
                   aclResource="Goomento_PageBuilder::import"
            />
        </referenceContainer>
    </body>
</page>
